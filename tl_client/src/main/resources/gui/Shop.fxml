<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="merchandiseHbox" prefHeight="537.0" prefWidth="800.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="at.ac.tuwien.inso.tl.client.gui.controller.PerformanceController">
  <children>
    <Label text="%label_availableArticles">
      <font>
        <Font name="System Bold" size="14.0" fx:id="x1" />
      </font>
    </Label>
    <TableView id="shopTableView" fx:id="articleTableView" prefHeight="248.0" prefWidth="460.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Name" fx:id="articleNameColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="115.0" text="%tableView_description" fx:id="articleDescriptionColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="%tableView_price" fx:id="articlePriceColumn" />
      </columns>
    </TableView>
    <Label text="%label_pieces" />
    <TextField fx:id="amountTextField" prefWidth="46.0" />
    <Button mnemonicParsing="false" onAction="#onSelectCartButtonClicked" prefWidth="134.0" text="%button_intoCart" />
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="15.0" />
  </padding>
</HBox>
